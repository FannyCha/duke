project
	: requirements
		<library>/dukexcore//dukexcore/<link>static			# embedded
		<library>/qt//QtCore/<variant>release/<link>shared
		<library>/qt//QtGui/<variant>release/<link>shared
	: usage-requirements
		<library>/qt//QtCore/<variant>release/<link>shared
		<library>/qt//QtGui/<variant>release/<link>shared
		<source>mainwindow.ui
	;

explicit dukexgui ;
lib dukexgui
    : 
        [ glob *.ui *.cpp *.qrc *.h : IUIBuilder.h IUIPluginInterfaces.h UIPluginLoader.h ] 
	;
